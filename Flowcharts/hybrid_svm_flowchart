digraph {
	rankdir=TB
	Title [label="Hybrid SVM Class" shape=plaintext]
	E [label="Dimensionality Reduction (PCA)" shape=box]
	F [label="Initialize Quantum Kernel" shape=box]
	G [label="Hyperparameter Tuning (SVM)" shape=box]
	H [label="Train Hybrid SVM" shape=box]
	I [label="Evaluate Model Performance" shape=box]
	J [label="Save Trained Model" shape=parallelogram]
	Title -> E [style=invis]
	E -> F
	F -> G
	G -> H
	H -> I
	H -> J
}
