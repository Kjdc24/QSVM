// QSVM Flowchart
digraph {
	rankdir=TB
	Title [label="QSVM Class" shape=plaintext]
	E [label="Dimensionality Reduction (PCA)" shape=box]
	F [label="Create Quantum Feature Map(ZZFeatureMap)" shape=box]
	G [label="Intialize Quantum Kernel" shape=box]
	H [label="Train Quantum Support Vector Machine" shape=box]
	I [label="Evaluate Model Performance" shape=box]
	J [label="Save Trained Model" shape=parallelogram]
	Title -> E [style=invis]
	E -> F
	F -> G
	G -> H
	H -> I
	H -> J
}
